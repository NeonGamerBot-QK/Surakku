{
  "manifest_version": 3,
  "name": "Surakku",
  "description": "Slack Client Mod",
  "author": "Neon <neon@saahild.com>",
  "host_permissions": [
    "*://*.app.slack.com/*",
    "https://raw.githubusercontent.com/*"
  ],
  "permissions": ["tabs", "cookies"],
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["*://*.app.slack.com/*"],
      "js": ["app/index.js"],
      "all_frames": true,
      "world": "ISOLATED"
    }
  ]
}
